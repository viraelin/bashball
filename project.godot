"class": "Ball",
"language": "GDScript",
"path": "res://src/Arena/Ball.gd"
}, {
"base": "Reference",
"class": "InputType",
"language": "GDScript",
"path": "res://src/UI/InputType.gd"
}, {
"base": "Reference",
"class": "Math",
"language": "GDScript",
"path": "res://src/Math.gd"
}, {
"base": "RigidBody2D",
"class": "Player",
"language": "GDScript",
"path": "res://src/Player/Player.gd"
}, {
"base": "Player",
