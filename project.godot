}

[application]

config/name="BashBall"
run/main_scene="res://src/Main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="viraelin/BashBall"
boot_splash/image="res://res/splash.png"
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://res/icon/icon.png"
config/macos_native_icon="res://res/icon/icon.icns"
config/windows_native_icon="res://res/icon/icon.ico"

[audio]

default_bus_layout="res://src/default_bus_layout.tres"

[autoload]

Game="*res://src/Autoload/Game.gd"
Audio="*res://src/Autoload/Audio.gd"
Effects="*res://src/Autoload/Effects.tscn"
Debug="res://src/Autoload/Debug.gd"

[debug]

gdscript/warnings/return_value_discarded=false

[display]

window/size/width=1280
window/size/height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor]

convert_text_resources_to_binary_on_export=true

[filesystem]

import/fbx/use_fbx=false

